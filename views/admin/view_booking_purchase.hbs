
<table class="table .table1">
  <thead class="t_head">
        <tr>
      <th scope="col">Name</th>
      <th scope="col">Price</th>
      <th scope="col">Category</th>
      <th scope="col">Pincode</th>
      <th scope="col">Date</th>
      <th scope="col">Address</th>
      
     

      
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <td>{{this.Name}}</td>
      <td>{{this.Price}}</td>
      <td>{{this.Category}}</td>
      <td>{{this.Pincode}}</td>
      <td>{{this.Address}}</td>
       </tr>{{/each}}
  </tbody>
</table>
